{
  "manifest_version": 2,
  "name": "Apollo",
  "version": "1.0",

  "description": "Sends links to an Apollo server to download.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://soundcloud.com/*"],
      "js": ["dist/index.js"]
    }
  ]

}
